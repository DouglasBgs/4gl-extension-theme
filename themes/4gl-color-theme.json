{
  "name": "4gl",
  "type": "dark",
  "colors": {
    "editor.foreground": "#fce4a1"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment", "comment.block"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#546e7a"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#fffdfd"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color", "constant.language"],
      "settings": {
        "foreground": "#fff1f1"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "functions",
      "scope": ["define.functions"],
      "settings": {
        "foreground": "#c792ea"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#25da61be"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method",
        "function.name"
      ],
      "settings": {
        "foreground": "#fce940"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "foreach",
      "scope": ["foreach", "define.for"],
      "settings": {
        "foreground": "#7b8fc2"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#29080a"
      }
    },
    {
      "name": "support.type.source.function",
      "scope": ["support.type.source.function"],
      "settings": {
        "foreground": "#71d7f0",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Database Variables",
      "scope": ["sintaxe.database", "storage.type.source.variables"],
      "settings": {
        "foreground": "#8e7cc3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Database Variables",
      "scope": "define.globals",
      "settings": {
        "foreground": "#dd7e6b"
      }
    },
    {
      "name": "Initialize",
      "scope": "define.initialize",
      "settings": {
        "foreground": "#f0f0f0"
      }
    },
    {
      "name": "define Variables",
      "scope": "define.variables",
      "settings": {
        "foreground": "#e07918"
      }
    },
    {
      "name": "then",
      "scope": "define.then",
      "settings": {
        "foreground": "#6aa84f"
      }
    },
    {
      "name": "inputs",
      "scope": "sintaxe.input",
      "settings": {
        "foreground": "#3aa87a"
      }
    },
    {
      "name": "constant numeric",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#80c8e4"
      }
    },
    {
      "name": "variable.name",
      "scope": "variable.name",
      "settings": {
        "foreground": "#f5f5f5"
      }
    },
    {
      "name": "seletores ",
      "scope": "end.if",
      "settings": {
        "foreground": "#b3588d"
      }
    },
    {
      "name": "while ",
      "scope": "window.display",
      "settings": {
        "foreground": "#485abe"
      }
    },
    {
      "name": "display ",
      "scope": "define.while",
      "settings": {
        "foreground": "#39943d"
      }
    },
    {
      "name": "chamadas de funções",
      "scope": "call.return",
      "settings": {
        "foreground": "#82dcff"
      }
    },
    {
      "name": "definir variavle let",
      "scope": "define.let",
      "settings": {
        "foreground": "#c792ea"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": ["meta.function", "sintaxe.bolean"],
      "settings": {
        "foreground": "#c4584a"
      }
    },
    {
      "name": "define.substring",
      "scope": "define.substring",
      "settings": {
        "foreground": "#FF0000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string.double",
        "string.single",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
        "variable.parameter.function"
      ],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name",
        "keyword.other"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Case",
      "scope": "define.case",
      "settings": {
        "foreground": "#9d48a8"
      }
    },
    {
      "name": "substring",
      "scope": "define.substring.4gl",
      "settings": {
        "foreground": "#f60000"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["url", "link", "uri"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "URL",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
